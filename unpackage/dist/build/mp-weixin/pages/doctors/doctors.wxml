<view><view class="table"><view class="table_header"><view class="th1"><view data-event-opts="{{[['tap',[['textChange',['$event']]]]]}}" class="{{['cell_label',colorFlag==true?'colorFlag':'']}}" bindtap="__e">全部时间</view></view><scroll-view style="width:100%;white-space:nowrap;" scroll-x="true"><block wx:for="{{dateArray}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['doctorFilter',['$0'],[[['dateArray','',index,'weekNum']]]]]]]}}" class="th2" style="width:100rpx;display:inline-block;" bindtap="__e"><view class="{{['cell_date_label',selectId==item.weekNum?'colorFlag':'']}}">{{item.date_text}}</view><view class="{{['cell_label',selectId==item.weekNum?'colorFlag':'']}}">{{item.weekName}}</view></view></block></scroll-view></view></view><scroll-view class="doctors" scroll-y="true"><block wx:for="{{doctor}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="itemdot" data-did="{{item.d_Id}}" data-dname="{{item.d_Name}}" data-dcount="{{item.d_Count}}" data-dreceive="{{item.d_ReceiveNum}}" data-dfee="{{item.d_Fee}}" data-drole="{{item.d_Role}}" data-introduce="{{item.d_Introduce}}" data-dimg="{{item.d_Img}}" data-event-opts="{{[['tap',[['getDoctors',['$event']]]]]}}" bindtap="__e"><view class="top"><view class="left"><image style="width:180rpx;height:180rpx;" src="{{item.d_Img}}"></image></view><view class="right"><view class="top1"><text style="font-size:35rpx;color:#0C0C0C;">{{item.d_Name}}</text><text>{{''+item.d_Role}}</text><block wx:if="{{item.d_ReceiveNum<30}}"><view class="youHao">有号</view></block><view class="wuHao" wx:else>无号</view></view><view class="bottom1"><text>{{"接诊总量 "+item.d_Count}}</text><text decode="{{true}}">{{'已接诊 '+item.d_ReceiveNum}}</text></view></view></view><view class="bottom">{{''+item.d_Introduce+''}}</view></view></block></scroll-view></view>