<view><scroll-view class="container" scroll-y="{{true}}"><block wx:for="{{guaHao}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item" data-gid="{{item.g_Id}}" data-gorder="{{item.g_Order}}" data-event-opts="{{[['tap',[['guaHaoDetail',['$event']]]]]}}" bindtap="__e"><view class="top"><text>预约详情</text><block wx:if="{{item.g_PayStatus==1}}"><text style="color:#676565;">预约成功</text></block></view><view class="center1"><image src="../../static/images/doctor1.png"></image><view class="right"><text><text style="color:#080808;">{{item.d_Name}}</text>{{''+item.d_Role}}</text><text>{{item.h_Name+" | "+item.room_Name}}</text></view></view><view class="center2"><text>{{"病情："+item.g_Introduce}}</text><text>{{"就诊时间："+item.g_Time}}</text><text>{{"门诊费用：￥"+item.d_Fee}}</text><block wx:if="{{item.g_PayStatus==0}}"><text>支付状态：未支付</text></block><text wx:else>支付状态：已支付</text></view><view class="bottom"><block wx:if="{{item.g_PayStatus==0}}"><view class="cancel" data-did="{{item.d_Id}}" data-gweek="{{item.g_Time}}" data-gtime="{{item.time_Period}}" data-gid="{{item.g_Id}}" data-event-opts="{{[['tap',[['cancelGuaHao',['$event']]]]]}}" catchtap="__e">取消挂号</view></block><block wx:if="{{item.g_PayStatus==0}}"><view class="pay" data-gid="{{item.g_Id}}" data-gweek="{{item.g_Time}}" data-event-opts="{{[['tap',[['PayGuaHao',['$event']]]]]}}" catchtap="__e">支付</view></block><block wx:if="{{item.g_PayStatus==1}}"><view class="payFinish">支付完成</view></block></view></view></block></scroll-view></view>